{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.sln" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "cwd": "src"
        }
      ],
      "dest": "tmp"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "cwd": "tmp",
        "dest": "doc"
      }
    ],
    "globalMetadata": {
      "_appTitle": "docfx seed website",
      "_enableSearch": true
    },
    "dest": "_site"
  }
}
